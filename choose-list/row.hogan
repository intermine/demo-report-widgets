<td><input type="checkbox" /></td>
<td><span class="star"></span></td>
<td>
    <ul class="tags">
        {{#tags}}
            <li title="{{name}}"><span class="color" style="background-color:rgb({{rgb}})"></span></li>
        {{/tags}}
    </ul>
    <span class="title">{{name}}</span>
    <span class="description">{{description}}</span>
</td>
<td><span class="owner"></span></td>
<td><span class="created" title="{{dateCreated}}">{{timeAgo}}</span></td>