<td><input type="checkbox" {{#selected}}checked="checked"{{/selected}}/></td>
<td>
    <ul class="tags">
        {{#tags}}
            <li data-model="{{id}}" data-slug="{{slug}}">
                <span class="color" style="background-color:{{color}}"></span>
            </li>
        {{/tags}}
    </ul>
    <span class="title">{{name}}</span>
    <span class="description">{{description}}</span>
</td>
<td><span class="created" title="{{prettyDate}}">{{timeAgo}}</span></td>