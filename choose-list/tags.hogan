<div class="select">Select <span class="all">all</span>/<span class="none">none</span></div>
<ul>
{{#tags}}
    <!-- active tag? -->
    {{#active}}
        <li><span class="radius label" style="background-color:rgb({{rgb}})">{{id}}</span></li>
    {{/active}}

    <!-- not an active tag :( -->
    {{^active}}
        <li><span class="radius label" style="background-color:rgba({{rgb}}, 0.5)">{{id}}</span></li>
    {{/active}}
{{/tags}}
</ul>